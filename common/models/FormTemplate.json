{
  "name": "FormTemplate",
  "options": {
    "idInjection": true,
    "mongodb": {
      "collection":"formTemplates"
    }
  },
  "properties": {
    "name": {
      "type": "String",
      "required": true
    },
    "template": {
      "type": "Boolean",
      "required": true
    },
    "createdDate": {
      "type": "Date"
    },
    "updatedDate": {
      "type": "Date"
    }
  },
  "relations": {
    "person": {
      "type":"belongsTo",
      "model":"Person",
      "foreignKey":"ownerId"
    }
  }
}
